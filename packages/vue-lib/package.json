{
  "name": "@demotmg/vue-lib",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@demotmg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/@demotmg/vue-lib"
  },
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "main": "./dist/uiLib.umd.cjs",
  "module": "./dist/uiLib.js",
  "exports": {
    ".": {
      "import": "./dist/uiLib.js",
      "require": "./dist/uiLib.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "vue": "^3.4.29",
    "@demotmg/multy-source": "^0.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.0",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.21"
  }
}
